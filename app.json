{
  "expo": {
    "name": "BSafe",
    "slug": "BSafe",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/bsafe.png",
    "splash": {
      "image": "./assets/load.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.Ampersand.BSafe",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "API_KEY"
        }
      }
    },
    "packagerOpts": {
      "assetExts": [
        "ttf"
      ]
    }
  }
}
